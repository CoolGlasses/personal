#homeBody {
    display: grid;
    grid-template-rows: auto auto auto auto;
    justify-content: center;
}

#topHomeContainer {
    display: grid;
    grid-row: 1 / 2;
    grid-template-columns: auto auto;
    grid-template-rows: auto auto;
}

#nameHeader {
    font-family: 'Times New Roman', Times, serif;
    grid-column: 1 / 2;
    grid-row: 1 / 2;
}

#mainPortrait {
    grid-column: 2 / 3;
    grid-row: 1 / 3;
    border-width: 10px;
    border-style: double;
    border-color: green;
    border-collapse: separate;
}

#coreValues {
    grid-column: 1 / 2;
    grid-row: 2 / 3;
}

#homeNavBar {
    display: grid;
    grid-row: 2 / 3;
    grid-template-columns: auto auto;
    background-color: gray;
}

#softwareProjectsButton {
    display: grid;
    grid-column: 1 / 2;
}

#coachAButton {
    display: grid;
    grid-column: 2 / 3;
}

#availabilityBar {
    grid-row: 3 / 4;
}

#softwareProjectsBlurb {
    display: grid;
    grid-row: 4 / 5;
}

#coachABlurb {
    display: none;
    grid-row: 4 / 5;
}


